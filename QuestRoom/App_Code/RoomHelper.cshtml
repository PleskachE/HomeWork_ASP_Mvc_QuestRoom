@using QuestRoom.Data.Entity

@helper HtmlList(IEnumerable<Picture> pictures)
{
    foreach (var item in pictures)
    {
        if (item.Logo == false)
        {
            <div style="display:inline-block; margin:10px">
                <img src=/@item.Path style="width:200px; border:1px solid black" />
            </div>
        }
    }
}

